    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "graphql-tag": "^2.12.6",
    "kafkajs": "^2.2.4",
    "nodemon": "^3.1.9"
  },
  "devDependencies": {
    "@apollo/client": "^3.12.6",
    "@apollo/server": "^4.11.3",
    "@prisma/client": "^6.2.1",
